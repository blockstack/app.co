{
  "presets": ["next/babel"],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./components",
          "@containers": "./containers",
          "@stores": "./stores",
          "@pages": "./pages",
          "@common": "./common",
          "@utils": "./utils",
          "@models": "./models"
        }
      }
    ]
  ]
}
